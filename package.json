{
  "name": "@iomete/jupyter-renderer-react",
  "version": "1.0.1",
  "description": "React component library for IOMETE Jupiter parser",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/index.css": "./dist/esm/index.css",
    "./dist/esm/index.css": "./dist/esm/index.css",
    "./dist/cjs/index.css": "./dist/cjs/index.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "test": "vitest",
    "test:watch": "vitest watch",
    "storybook": "storybook dev -p 6006",
    "storybook:clean": "pkill -f 'storybook dev' || true && rm -rf node_modules/.cache/storybook",
    "build-storybook": "storybook build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "iomete",
    "jupiter",
    "parser",
    "component"
  ],
  "author": "Mammad Mammadli",
  "license": "MIT",
  "type": "module",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-onboarding": "^9.1.8",
    "@storybook/react-vite": "^9.1.8",
    "@types/marked": "^5.0.2",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.52.3",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^9.1.8",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iomete/jupyter-renderer-react"
  },
  "bugs": {
    "url": "https://github.com/iomete/jupyter-renderer-react/issues"
  },
  "homepage": "https://github.com/iomete/jupyter-renderer-react#readme",
  "dependencies": {
    "@jupyterlab/codemirror": "^4.4.10",
    "@jupyterlab/theme-dark-extension": "^4.4.10",
    "@jupyterlab/theme-light-extension": "^4.4.10",
    "@jupyterlab/ui-components": "^4.4.10",
    "marked": "^16.4.1"
  }
}
